my_dict = {
    "name": "caca",
    "age": 18,
    "city": "Indonesia"
}

print(my_dict["city"])

my_dict["job"] = "Engineer"

my_dict["age"] = 18

del my_dict["city"]

for key, value in my_dict.items():
    print(key, value)

if "name" in my_dict:
    print("Name exists!")

age = my_dict.get("age", "Unknown")
